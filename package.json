{
  "name": "sts-app",
  "version": "0.0.1",
  "description": "Simple Ticket System Application",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test-server": "./node_modules/.bin/mocha \"server/tests/**/*.test.js\" --watch --recursive --require babel-register",
    "start-server": "nodemon --watch server --exec babel-node -- server/index.js",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --display-error-details --open",
    "build": "./node_modules/.bin/webpack --progress --env=prod --define process.env.NODE_ENV='production'"
  },
  "jest": {
    "testPathDirs": [
      "client/tests"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.scss?$": "./client/tests/scss_preprocessor"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "ticket",
    "sts",
    "clent"
  ],
  "author": "michaljabi",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.26.1",
    "jest": "^18.1.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.4.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "latest",
    "express": "latest",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-recaptcha": "^2.2.6",
    "react-router": "^3.0.2",
    "validator": "^6.2.1"
  }
}
